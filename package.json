{
  "name": "CacheTour",
  "version": "0.1.0",
  "description": "Collect Geocaches from geocaching.com and download them as single GPX file.",
  "main": "dist/CacheTour.user.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Rocka84 <f.dillmeier@gmail.com>",
  "userscript": {
  	  "namespace": "de.rocka84.cachetour",
  	  "include": [
  	  	  "http*://www.geocaching.com/*"
  	  ],
  	  "exclude": [
  	  	  "https://www.geocaching.com/articles",
  	  	  "https://www.geocaching.com/about",
  	  	  "https://www.geocaching.com/login"
  	  ],
  	  "grant": [
  	  	  "GM_getValue",
  	  	  "GM_setValue",
  	  	  "GM_deleteValue",
  	  	  "GM_log",
  	  	  "GM_addStyle",
  	  	  "GM_xmlhttpRequest",
  	  	  "GM_getResourceText",
  	  	  "GM_getResourceURL"
  	  ],
  	  "require" : [
  	  	  "https://raw.githubusercontent.com/eligrey/FileSaver.js/master/FileSaver.min.js"
      ],
  	  "update_url": "https://github.com/Rocka84/CacheTour/raw/master/CacheTour.version.js",
  	  "download_url": "https://github.com/Rocka84/CacheTour/raw/master/CacheTour.user.js",
  	  "icon": "https://github.com/Rocka84/CacheTour/raw/master/icon.png",
  	  "run_at": "document-end"
  },
  "license": "ISC",
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-notify": "^0.4.5"
  }
}
